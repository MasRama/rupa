{
  "name": "discord-bot-starter",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript Discord bot starter project with database integration and modern development practices",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "migrate": "ts-node src/services/database/migrate.ts",
    "deploy-commands": "ts-node src/deploy-commands.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "discord.js",
    "knex",
    "sqlite"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "knex": "^3.1.0",
    "better-sqlite3": "^9.2.2",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.10.6",
    "@types/better-sqlite3": "^7.6.8",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}